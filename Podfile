source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!
inhibit_all_warnings!

target 'NotAnotherStarWarsApp' do
   pod 'Reusable'
   pod 'Alamofire', '~> 4.0'
   pod 'Alamofire-Synchronous', '~> 4.0'
   pod 'ObjectMapper'
   pod 'HydraAsync'
   pod 'ResponseDetective', '~> 1.0'
   pod 'Fabric'
   pod 'Crashlytics'
end

target 'NotAnotherStarWarsAppTests' do
	pod 'Reusable'
end

target 'NotAnotherStarWarsAppUITests' do
   pod 'XCTest-Gherkin'
   pod 'XCTest-Gherkin/Native'
end
